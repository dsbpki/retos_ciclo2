// First challenge
package Aseguradora;

public class main {

	public static void main(String[] args) {
		int workedDays= 150;
		Empleado empleado1 = new Empleado("david",1810050);
		Empleado empleado2 = new Empleado("juan", 2000000);
		
		System.out.println("El empleado: "+ empleado1.getNombre()+ " gana: "+ empleado1.getSalario());
		
		System.out.println("las prestaciones son: "+ Aseguradora.liquidarPrestaciones(empleado1, workedDays));
		System.out.println("La seguridad social es: "+Aseguradora.liquidarSegSocial(empleado1, workedDays));
		System.out.println("las prestaciones son2: "+ Aseguradora.liquidarPrestaciones(empleado2, workedDays));
		System.out.println("La seguridad social es2: "+Aseguradora.liquidarSegSocial(empleado2, workedDays));
		
		
	}

}
