// First challenge
package Aseguradora;

public class main {

	public static void main(String[] args) {
		int workedDays= 150;
		System.out.println("las prestaciones son: "+ Aseguradora.liquidarPrestaciones(1810050, workedDays));
		System.out.println("La seguridad social es: "+Aseguradora.liquidarSegSocial(1810050, workedDays));
		System.out.println("las prestaciones son2: "+ Aseguradora.liquidarPrestaciones(2000000, workedDays));
		System.out.println("La seguridad social es2: "+Aseguradora.liquidarSegSocial(2000000, workedDays));
		Empleado empleado1 = new Empleado("david",5000000);
		System.out.println("El empleado: "+ empleado1.getNombre()+ " gana: "+ empleado1.getSalario());
	}

}
